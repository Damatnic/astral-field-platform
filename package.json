{
  "name": "astral-field",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:coverage": "jest --config jest.integration.config.js --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "deploy": "bash scripts/deploy.sh",
    "deploy:prod": "bash scripts/deploy.sh --production",
    "deploy:verify": "node scripts/verify-deployment.js",
    "deploy:setup": "node scripts/setup-vercel.js",
    "health": "curl -f http://localhost:3000/api/health || curl -f http://localhost:3001/api/health",
    "db:setup": "curl -X POST http://localhost:3000/api/setup-users -H 'Authorization: Bearer astral2025' || curl -X POST http://localhost:3001/api/setup-users -H 'Authorization: Bearer astral2025'",
    "validate:env": "node -e \"require('./src/lib/env-validation.ts')\"",
    "build:analyze": "ANALYZE=true npm run build",
    "test:all": "npm run test && npm run test:e2e",
    "test:agents": "npm run test -- __tests__/agents",
    "progress:report": "node scripts/progress-tracker.js report",
    "progress:standup": "node scripts/progress-tracker.js standup",
    "progress:health": "node scripts/progress-tracker.js health",
    "agents:init": "node scripts/progress-tracker.js init",
    "agents:update": "node scripts/progress-tracker.js update",
    "build:production": "node scripts/production-build.js",
    "validate:production-env": "node scripts/validate-env.js",
    "deploy:staging": "npm run build:production && vercel",
    "deploy:production": "npm run build:production && vercel --prod",
    "monitor:production": "node scripts/monitor-deployment.js",
    "analyze": "ANALYZE=true next build",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output=html",
    "security:scan": "npm audit --audit-level=high",
    "optimize:images": "node scripts/optimize-images.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "test:production": "NODE_ENV=test npm run test -- --coverage --watchAll=false",
    "demo:setup": "node scripts/one-click-setup.js",
    "db:migrate": "node scripts/run-migrations.js",
    "db:seed": "node scripts/seed-demo-data.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@neondatabase/serverless": "^1.0.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.0",
    "@tanstack/react-query": "^5.86.0",
    "@types/pg": "^8.15.5",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/ws": "^8.18.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "openai": "^5.19.1",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-is": "^19.1.1",
    "recharts": "^3.1.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ws": "^8.18.3",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "msw": "^2.6.4",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.6.2",
    "supabase": "^2.39.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  }
}
